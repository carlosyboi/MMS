local a=game:GetService"Players"local b=game:GetService"Workspace"while not a.LocalPlayer do if task and task.wait then task.wait()else wait()end end;local c=a.LocalPlayer;local d=b.CurrentCamera;local e=0x148;local f=0xC0;local g=false;local function h()print"Automatic Offset Scan..."local i=c.Character;if not i then print"Scan pause, wait for spawn"return end;local j=i:FindFirstChild"HumanoidRootPart"if not j then return end;local k=j.Address;local l=j.Position;local function m(n,o)local p=memory_read("float",n)local q=memory_read("float",n+4)local r=memory_read("float",n+8)if not p or not q or not r then return false end;local s=p-o.X;local t=q-o.Y;local u=r-o.Z;return s*s+t*t+u*u<0.01 end;for n=0,0x500,4 do local o,p=pcall(function()return memory_read("uintptr_t",k+n)end)if o and type(p)=="number"and p>0x100000 then for q=0,0x200,4 do if m(p+q,l)then e=n;f=q-0x24;print(string.format("good scan result. Primitive: 0x%X, CFrame: 0x%X",e,f))g=true;return end end end end;print"Scan Failed. Using defaults."g=true end;task.spawn(h)local function v(i)if not i or i==0 then return nil end;local j,k=pcall(function()local j=memory_read("uintptr_t",i+e)if not j or j==0 then return nil end;local k=j+f;local l=memory_read("float",k+0x08)local n=memory_read("float",k+0x14)local o=memory_read("float",k+0x20)if type(l)=="number"then return Vector3.new(-l,-n,-o)end end)return j and k or nil end;local i={}function makeEsp(j)local k={}local l=Drawing.new"Square"l.Visible=false;l.Color=Color3.fromRGB(255,0,0)l.Thickness=2;l.Transparency=1;l.Filled=false;l.ZIndex=999;local n=Drawing.new"Text"n.Visible=false;n.Color=Color3.fromRGB(255,255,255)n.Center=true;n.Outline=true;n.Size=16;n.ZIndex=999;n.Text=j.Name;local o=Drawing.new"Line"o.Visible=false;o.Color=Color3.fromRGB(0,255,0)o.Thickness=2;o.ZIndex=999;function k.remove()l:Remove()n:Remove()o:Remove()end;function k.update()if not j.Parent or not j.Character then l.Visible=false;n.Visible=false;o.Visible=false;return true end;local p=j.Character:FindFirstChild"HumanoidRootPart"or j.Character:FindFirstChild"Torso"if not p then return true end;local q=p.Position;local r=q+Vector3.new(0,3,0)local s=q-Vector3.new(0,3,0)local t,u=WorldToScreen(r)local w,x=WorldToScreen(s)if u or x then local y=math.abs(t.Y-w.Y)local z=y/1.5;l.Size=Vector2.new(z,y)l.Position=Vector2.new(t.X-z/2,t.Y)l.Visible=true;n.Position=Vector2.new(t.X,w.Y+2)n.Visible=true;local A=v(p.Address)if A then local B=15;local C=Vector3.new(A.Y*0-A.Z*1,A.Z*0-A.X*0,A.X*1-A.Y*0)local D=math.sqrt(C.X^2+C.Y^2+C.Z^2)local E=D>0 and Vector3.new(C.X/D,C.Y/D,C.Z/D)or Vector3.new(1,0,0)local F=Vector3.new(E.Y*A.Z-E.Z*A.Y,E.Z*A.X-E.X*A.Z,E.X*A.Y-E.Y*A.X)local G=math.sqrt(F.X^2+F.Y^2+F.Z^2)local H=G>0 and Vector3.new(F.X/G,F.Y/G,F.Z/G)or Vector3.new(0,1,0)local I=E*1.5+H*1.0;local J=q+I;local K,L=WorldToScreen(J)local M,N=WorldToScreen(J+A*B)local function O(P)return P.X and P.Y and(P.X~=0 or P.Y~=0)and P.X==P.X end;if(L or N)and O(K)and O(M)then o.From=K;o.To=M;o.Visible=true else o.Visible=false end else o.Visible=false end else l.Visible=false;n.Visible=false;o.Visible=false end;return true end;return k end;while true do for j,k in ipairs(a:GetChildren())do if k.Name~=c.Name and not i[k.Name]then i[k.Name]=makeEsp(k)end end;for j,k in pairs(i)do local l=pcall(k.update)if not l then k.remove()i[j]=nil end end;if task and task.wait then task.wait()else wait()end end
